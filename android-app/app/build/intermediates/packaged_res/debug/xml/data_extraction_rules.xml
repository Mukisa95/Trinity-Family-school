<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude WebView cache and data from cloud backup -->
        <exclude domain="database" path="webview.db"/>
        <exclude domain="database" path="webviewCache.db"/>
        <exclude domain="file" path="webview_cache"/>
        <exclude domain="file" path="webview_data"/>
    </cloud-backup>
    <device-transfer>
        <!-- Exclude WebView cache and data from device transfer -->
        <exclude domain="database" path="webview.db"/>
        <exclude domain="database" path="webviewCache.db"/>
        <exclude domain="file" path="webview_cache"/>
        <exclude domain="file" path="webview_data"/>
    </device-transfer>
</data-extraction-rules>
